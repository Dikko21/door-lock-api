<!DOCTYPE html>
<html>

<head>
    <title>Log History</title>
    <style>
        body {
            font-size: 14px;
            font-family: Inter, -apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji";
            line-height: 1.5;
            color: rgb(17, 25, 39);
            background-color: rgb(243, 244, 246);
        }

        .wrapper {
            background-color: white;
            width: 700px;
            margin: 24px auto;
            padding-bottom: 24px;
            border-radius: 16px;
            box-shadow: rgba(0, 0, 0, 0.04) 0px 5px 22px, rgba(0, 0, 0, 0.03) 0px 0px 0px 0.5px;
        }

        .title {
            font-size: 24px;
            font-weight: bold;
            text-align: center;
            padding: 24px 0 24px 0;
        }

        table {
            width: 700px;
            border-spacing: 0;
            margin: auto;
        }

        thead {
            background: rgb(248, 249, 250);
            letter-spacing: 0.5px;
            text-transform: uppercase;
            font-weight: 600;
        }

        tbody {
            font-weight: 400;
        }

        tbody tr td {
            border-bottom: 1px solid rgb(242, 244, 247);
        }

        tbody tr:hover {
            background-color: rgba(17, 25, 39, 0.04);
        }

        td {
            padding: 15px 16px;
        }
    </style>
</head>

<body>
    <div class="wrapper">
        <div class="title">Data Log History</div>
        <table>
            <thead>
                <tr>
                    <td>#</td>
                    <td>Username</td>
                    <td>Status</td>
                    <td style="text-align: right;">Login Date</td>
                </tr>
            </thead>
            <tbody>
                <% variableName.forEach(function(data){ %>
                    <tr>
                        <td>
                            <%= data.no %>
                        </td>
                        <td>
                            <%= data.username %>
                        </td>
                        <td>
                            <%= data.status %>
                        </td>
                        <td style="text-align: right;">
                            <%= data.loginDate %>
                        </td>
                    </tr>
                    <% }); %>
            </tbody>
        </table>
    </div>
</body>

</html>